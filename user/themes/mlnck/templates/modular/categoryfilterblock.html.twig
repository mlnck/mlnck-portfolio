<div class="category-filters">
{% for k,v in site.categoryfilters %}
  <div class="category-filter" data-category="{{strtolower(k)}}">
    {% if(k == 'Tags') %}
      {{ site.svgtagicon }}
    {% else %}
      <div class="fleck -{{ strtolower(k) }} -{{ strtolower(k) }}"></div>
    {% endif %}
    <span class="filter">{{ strtolower(k) }}</span>
  </div>
{% endfor %}

</div>

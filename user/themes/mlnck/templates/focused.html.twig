{% extends 'partials/base.html.twig' %}
{% set collection = page.collection() %}
{% block content %}
    {% for module in collection %}
      {% if module.slug == '_timeline' %}
        <aside class="timeline-settings">
          {{ module.content }}
        </aside>
      {% elseif module.slug == '_focused' %}
        <main class="content-wrapper">
          <article class="article">
            {{ module.content }}
          </article>
        </main>
      {% endif %}
      {{dump(module)}}
    {% endfor %}
{% endblock %}

{% block collapsedNav %}
{% for module in collection %}
  {% if module.slug == '_categoryfilter' %}
    {{ module.content }}
  {% endif %}
{% endfor %}
{% endblock %}
